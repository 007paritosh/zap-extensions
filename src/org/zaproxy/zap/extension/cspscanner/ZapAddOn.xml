<zapaddon>
	<name>Content Security Policy Scanner</name>
	<version>3</version>
	<status>alpha</status>
	<description>Content Security Policy (CSP) Scanner</description>
	<author>ZAP Dev Team</author>
	<dependencies>
		<javaversion>1.8</javaversion>
	</dependencies>
	<url/>
	<changes>
	<![CDATA[
		Upgraded to Salvation library version 2.3.0.</br>
		Add support for checking if worker-src allows wildcard sources.<br>
	]]>
	</changes>
	<extensions>
		<extension>org.zaproxy.zap.extension.cspscanner.ExtensionContentSecurityPolicyScanner</extension>
	</extensions>
	<ascanrules/>
	<pscanrules>
		<pscanrule>org.zaproxy.zap.extension.cspscanner.ContentSecurityPolicyScanner</pscanrule>
	</pscanrules>
	<filters/>
	<files/>
	<not-before-version>2.5.0</not-before-version>
	<not-from-version></not-from-version>
</zapaddon>
