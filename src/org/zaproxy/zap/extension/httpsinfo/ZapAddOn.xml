<zapaddon>
	<name>HttpsInfo</name>
	<version>9</version>
	<status>alpha</status>
	<description>Displays HTTPS configuration information.</description>
	<author>ZAP Dev Team</author>
	<dependencies>
		<javaversion>1.8</javaversion>
	</dependencies>
	<url></url>
	<changes>
	<![CDATA[
	Switch from SSLServer to OWASP DeepViolet.<br>
	]]>
	</changes>
	<extensions>
	    <extension>org.zaproxy.zap.extension.httpsinfo.ExtensionHttpsInfo</extension>
	</extensions>
	<ascanrules/>
	<pscanrules/>
	<filters/>
	<files/>
	<not-before-version>2.4.0</not-before-version>
	<not-from-version></not-from-version>
</zapaddon>

